  <body style=" background-color: #EAEBEC;">
    <header>
      <img alt="Icon" src="/assets/u3.png" style="width:100%"/>     
    </header>
     
    <div id="left" style="width: 33%;float: left;">
      <img alt="Icon" src="/assets/u1.png" style="width:90%"/>
    </div>
    <div id="right" style="width: 67%;float: right; background-color: #FFFFFF;">
      <div >
         <span class="bg-primary" style="display:block;margin:auto;width:100px;text-align: center;font-size: 18px;margin-top:30px">评测报告</span >
      <textarea rows="20" cols="20" style="margin-top:20px;margin-left:40px;width: 89%;height: 70px;resize:none;height:500px" readonly="readonly">
         <%='ruby version:' +@data[:version] %> 
         <% for x in @data[:examples] %>
         <%= x%>
         <%end%>
      
      </textarea>
      </div>

     <table class="table table-striped" style="width: 90%;margin-left: 20px;">
      <thead>
      <tr>
         <th>学号</th>
         <th>姓名</th>
         <th>代码</th>
         <th>评测</th>
         <th>得分</th>
      </tr>
   </thead>
   <tbody>
      <tr>
         <td>201528015059072 </td>
         <td>任杰</td>
         
         <td> 
            <button type="button" class="btn btn-default btn-xs">查看提交代码</button>
         </td>
         <td> 
          <button type="button" class="btn btn-default btn-xs">查看测试代码</button>
         </td>
         <td>90.0</td>

      </tr>
   </tbody>
</table>
       
    </div>

  <footer>
